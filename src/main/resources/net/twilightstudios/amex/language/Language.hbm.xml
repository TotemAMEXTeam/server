<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
        
<hibernate-mapping>

	<class name="net.twilightstudios.amex.language.entity.Language" table="language">
	
		<id name="code" type="java.lang.String" column="code">
			<generator class="assigned"/>
		</id>	
		
		<property name="language" type="java.lang.String" not-null="true" column="language"/>
	
	</class>

</hibernate-mapping>