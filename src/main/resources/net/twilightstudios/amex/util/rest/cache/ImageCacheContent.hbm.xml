<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
        
<hibernate-mapping>

	<class name="net.twilightstudios.amex.util.rest.cache.entity.ImageCacheContent" table="image_cache_content">
	
		<id name="id" type="java.lang.Long" column="id">
			<generator class="identity"/>
		</id>	
		
		<property name="url" type="text" not-null="true" column="url"/>
		<property name="timestamp" type="java.util.Date" not-null="true" column="timestamp"/>
		<property name="image" type="blob" not-null="true" column="image"/>
	
	</class>

</hibernate-mapping>