<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
        
<hibernate-mapping>

	<class name="net.twilightstudios.amex.geo.entity.Country" table="country">
	
		<id name="id" type="java.lang.Long" column="id">
			<generator class="identity"/>
		</id>	
		
		<property name="name" type="java.lang.String" not-null="true" column="name"/>
		<property name="standardCode" type="java.lang.String" not-null="true" column="standardCode"/>
		<property name="language" type="java.lang.String" not-null="true" column="language"/>
	
	</class>

</hibernate-mapping>